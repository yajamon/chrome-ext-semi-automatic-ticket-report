{
    "manifest_version": 2,
    "name": "semiAutomaticTicketReport",
    "version": "0.0.0",

    "description": "アイドルマスターシンデレラガールズ4th Liveに落選した者が転売チケットを目の当たりにして怒りの違反報告するための拡張。",
    "icons": {
        "16": "img/icons/icon16.png",
        "48": "img/icons/icon48.png",
        "128": "img/icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["http://auctions.yahoo.co.jp/jp/show/violation_report?*"],
            "js": ["js/yahooAuctionReport.js"]
        }
    ]
}
